{
	"$schema": "https://json.schemastore.org/prettierrc",
	"tabWidth": 2,
	"printWidth": 120,
	"useTabs": true,
	"semi": false,
	"vueIndentScriptAndStyle": true,
	"singleQuote": true,
	"quoteProps": "as-needed",
	"bracketSpacing": true,
	"trailingComma": "all",
	"jsxSingleQuote": false,
	"arrowParens": "always",
	"proseWrap": "never",
	"htmlWhitespaceSensitivity": "strict",
	"endOfLine": "auto"
}
